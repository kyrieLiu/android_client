<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/view_title_bar"/>
    <com.chinasoft.ctams.view.XListView
        android:id="@+id/lv_daily_office_plan"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        android:dividerHeight="0.5dp"
        android:divider="@color/divierColor">
    </com.chinasoft.ctams.view.XListView>
    <com.melnykov.fab.FloatingActionButton
        android:id="@+id/fab_daily_office_plan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_margin="16dp"
        android:src="@drawable/icon"
        fab:fab_type="normal"
        fab:fab_shadow="true"
        fab:fab_colorNormal="@color/mainColorBlue"
        fab:fab_colorPressed="@color/dark_main_bg"
        fab:fab_colorRipple="@color/divierColor"/>
</FrameLayout>